<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/bootstrap/base")
    @Styles.Render("~/Content/bootstrap/theme")
</head>
<body>
    <div class="container">

        <div class="masthead">
            <h3 class="text-muted">MBAC Nationals 2014 - Winnipeg</h3>
            <ul class="nav nav-justified">
                <li class="active"><a href="/">Home</a></li>
                <li><a href="#">Schedule</a></li>
                <li><a href="#">Sponsors</a></li>
                <li><a href="#">Store</a></li>
                <li><a href="#">Results</a></li>
                <li><a href="/Participant">Participants</a></li>
                <li><a href="#">Contact</a></li>
            </ul>
        </div>

        @RenderBody()

        <!-- Site footer -->
        <div class="footer">
            <p>&copy; <a href="http://gcsquared.com/">GC2</a> 2014</p>
        </div>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
